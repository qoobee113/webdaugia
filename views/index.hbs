<script>
    $('a', '.site-nav.vertical-nav').each(function(){
        if($(this).attr('class') === $(this).parents(".site-nav.vertical-nav").attr('id') )
        {
            $(this).parents("li").removeClass("hidden");
        }
    });
    $('.nav-item.icon.hidden').remove();
    $('.col-sm-6.col-md-4', '.col-lg-9.col-md-9.col-sm-9.col-xs-12.wrap-product.wrp_xs_pd').each(function(){
        var m = $(this).parent('.col-lg-9.col-md-9.col-sm-9.col-xs-12.wrap-product.wrp_xs_pd').attr('class');
        var x = m.slice(60, m.length);
        if($(this).attr('id') === x )
        {
            $(this).removeClass("hidden");
        }
    });
    $('.col-sm-6.col-md-4.hidden').remove()
</script>
<script>
    $('.time','.product-box.product_box_tab').each(function(){
        var m = $(this).attr('class').slice(5, $(this).attr('class').length);
        if(m === $(this).parents('.product-box.product_box_tab').attr('id'))
        {
            var timeFinish = $(this).attr('value');
            var calcNewYear = setInterval(function(){
                date_future = new Date(timeFinish);
                date_now = new Date();
                seconds = Math.floor((date_future - (date_now))/1000);
                minutes = Math.floor(seconds/60);
                hours = Math.floor(minutes/60);
                days = Math.floor(hours/24);
                hours = hours-(days*24);
                minutes = minutes-(days*24*60)-(hours*60);
                seconds = seconds-(days*24*60*60)-(hours*60*60)-(minutes*60)
                if(days < 0)
                {
                }
                $('.'+m).text((days) + " ngày, " + checkTime(hours) + ":" + checkTime(minutes) + ":" + checkTime(seconds));
            },1000);
        }
        function checkTime(i)
        {
            if (i < 10) {
                i = "0" + i;
            }
            return i;
        }
    });
    $('.time','.thumbnail').each(function(){
        var m = $(this).attr('class').slice(5, $(this).attr('class').length);
        var n = $(this).parents('.thumbnail').attr('id');
        if(m === n)
        {
            var timeFinish = $(this).attr('value');
            var calcNewYear = setInterval(function(){
                date_future = new Date(timeFinish);
                date_now = new Date();
                seconds = Math.floor((date_future - (date_now))/1000);
                minutes = Math.floor(seconds/60);
                hours = Math.floor(minutes/60);
                days = Math.floor(hours/24);
                hours = hours-(days*24);
                minutes = minutes-(days*24*60)-(hours*60);
                seconds = seconds-(days*24*60*60)-(hours*60*60)-(minutes*60)
                if(days < 0)
                {
                }
                $('.time.'+m).text((days) + " ngày, " + checkTime(hours) + ":" + checkTime(minutes) + ":" + checkTime(seconds));
            },1000);
        }
        function checkTime(i)
        {
            if (i < 10) {
                i = "0" + i;
            }
            return i;
        }
    });
</script>
